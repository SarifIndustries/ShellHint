<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="steiner.bisley.shellhint">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon1_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme1">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NetworkScanActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ShellCommandsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NSDetailsActivity"
            android:parentActivityName=".NetworkScanActivity" />
        <activity
            android:name=".PlusActivity"
            android:label="@string/plus_activity_logo"
            android:parentActivityName=".MainActivity"></activity>

    </application>

</manifest>